datatype nat
    z : nat
    s : nat -> nat

add : nat -> nat -> nat
add m n :=
    case m of
        nat.z => n
        nat.s k => nat.s (add k n)

add : nat -> nat -> nat
mul m n :=
    case m of
        nat.z => nat.z
        nat.s k => add (mul k n) n

age_of_yjsnpi := mul 6 4

id : |T:type| T -> T
id _ x := x
y := id ? $ nat.s nat.z